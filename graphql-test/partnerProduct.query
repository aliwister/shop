query partnerProduct {
    partnerProduct (id: 87303)  {
        id
        sku
        upc
        ref
        slug
        brand
        name
        image
        priceObj {
            amount
            currency
        }
        costObj {
            amount
            currency
        }
        weight
        variationType
        unit
        availability
        quantity
        discountInPercent
        gallery
        options {
            name
            label
            values
        }
        children {
            id
            slug
            image
            priceObj {
                amount
                currency
            }
            costObj {
                amount
                currency
            }
            weight
            availability
            quantity
            discountInPercent
            variationAttributes {
                name
                value
            }
        }
        langs {
            features
            lang
        }
    }
}